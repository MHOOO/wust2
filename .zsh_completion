_start() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="psql dev prod.http prod test.postgres test.integration test.prod.http test.prod local.prod.http local.prod"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -F _start start
