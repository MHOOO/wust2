_start() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="dev migrate psql [options] pgdump pgrestore file pgclean prod.http prod test test.postgres test.integration test.prod.http test.prod"

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -F _start start
